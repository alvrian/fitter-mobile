{
  "expo": {
    "name": "fitter-mobile",
    "slug": "fitter-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fitter_logo.webp",
    "scheme": "fittermobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/fitter_logo.webp",
      "resizeMode": "contain",
      "backgroundColor": "#2C2C2E"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fitter_logo.webp",
        "backgroundColor": "#2C2C2E"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6ea11217-d1d0-4c4e-976b-a51cf63c665a"
      }
    }
  }
}
