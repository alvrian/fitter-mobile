{
  "expo": {
    "name": "fitter-mobile",
    "slug": "fitter-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fitter_logo.webp",
    "scheme": "fittermobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/fitter_logo.webp",
      "resizeMode": "contain",
      "backgroundColor": "#2C2C2E"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fitter_logo.webp",
        "backgroundColor": "#2C2C2E"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-camera",{
        "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
        "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
        "recordAudioAndroid": true
      }]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
